{
  "hosting": {
    "public": "build",
    "cleanUrls": true,
    "headers": [
      {
        "source": "**/*",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      },
      {
        "source" : "**/*.@(jpg|jpeg|gif|png|svg)",
        "headers" : [
          {
            "key" : "Cache-Control",
            "value" : "max-age=2592000"
          }
        ]
      },
      {
        "source" : "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
        "headers" : [
          {
            "key" : "Cache-Control",
            "value" : "max-age=2592000"
          }
        ]
      }
    ],
    "ignore": [
      "firebase.json",
      "package.json",
      "README.md",
      "**/.*",
      "**/node_modules/**",
      "node_modules/**",
      "public/**",
      "src/**",
      "firebase_functions/**",
      ".git/**"
    ],
    "redirects": [
      {
        "source": "/whitelist",
        "destination": "/#/whitelist",
        "type": 301
      },
      {
        "source": "/marketplace",
        "destination": "/#/marketplace",
        "type": 301
      },
      {
        "source": "/dashboard",
        "destination": "/#/dashboard",
        "type": 301
      },
      {
        "source": "/asset/:assetId",
        "destination": "/#/asset/:assetId",
        "type": 301
      },
      {
        "source": "/deal/:assetId",
        "destination": "/#/deal/:assetId",
        "type": 301
      },
      {
        "source": "/history/:assetId",
        "destination": "/#/history/:assetId",
        "type": 301
      },
      {
        "source": "/faucet",
        "destination": "/#/faucet",
        "type": 301
      }
    ]
  }
}
